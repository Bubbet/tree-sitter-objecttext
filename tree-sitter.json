{
  "$schema": "https://tree-sitter.github.io/tree-sitter/assets/schemas/config.schema.json",
  "grammars": [
    {
      "name": "objecttext",
      "camelcase": "ObjectText",
      "title": "Object Text",
      "scope": "source.objecttext",
      "file-types": [
        "rules"
      ],
      "highlights": "queries/objecttext/highlights.scm",
      "injection-regex": "^objecttext$",
      "class-name": "TreeSitterObjecttext"
    }
  ],
  "metadata": {
    "version": "0.1.0",
    "license": "MIT",
    "description": "Halfling Engine's Object Text. Used in Cosmoteer.",
    "authors": [
      {
        "name": "Bubbet",
        "email": "objecttext@bubbet.dev",
        "url": "https://bubbet.dev/"
      }
    ],
    "links": {
      "repository": "https://github.com/bubbet/tree-sitter-objecttext"
    }
  },
  "bindings": {
    "c": true,
    "go": true,
    "node": true,
    "python": true,
    "rust": true,
    "swift": true,
    "zig": false
  }
}